/* Enable IPQ9574 integrated radio's reserved memory */
&q6_region {
	status = "ok";
};

&m3_dump {
	status = "ok";
};

&q6_etr_region {
	status = "ok";
};

&q6_caldb_region {
	status = "ok";
};

/* Enable the reserved memory regions of QCN9224 on PCIe0, PCIe2 */
&qcn9224_pcie0 {
	status = "ok";
};

&mhi_region0 {
	status = "ok";
};

&qcn9224_pcie1 {
	status = "disabled";
};

&mhi_region1 {
	status = "disabled";
};

&qcn9224_pcie2 {
	status = "ok";
};

&mhi_region2 {
	status = "ok";
};

&qcn9224_pcie3 {
	status = "disabled";
};

&mhi_region3 {
	status = "disabled";
};

/* MLO GLOBAL MEM REGION size is 12MB for RDP with 2 QCN224 radios */
&mlo_global_mem0 {
	size = <0x0 0xC00000>;
	status = "ok";
};

&wifi0 {
	/* Enable IPQ9574 integratd 2G Radio */
	led-gpio = <&tlmm 64 1>;
	qcom,board_id = <0x20>;
	status = "ok";
};

&wifi4 {
	hremote_node = <&qcn9224_pcie0>;
	board_id = <0x02>;
	/delete-property/ interrupt-bmap;
	status = "ok";
};

&wifi5 {
	/* QCN9224 2G on PCI1 is disabled for AL02-C19 */
	hremote_node = <&qcn9224_pcie1>;
	board_id = <0x01>;
	status = "disabled";
};

&wifi6 {
	hremote_node = <&qcn9224_pcie2>;
	board_id = <0x04>;
	status = "ok";
};

&wifi7 {
	hremote_node = <&qcn9224_pcie3>;
	board_id = <0x02>;
